@article{GaoBaojun,
  title={Analysis of the Impact of Online Evaluation System on Commodity Sales: Based on Data from Jingdong and Tmall},
  author={Gao Baojun and Wang Hanning and Huang Yan and Hou Yangyang},
  journal={Price theory and practice},
  volume={000},
  number={8},
  pages={103-105},
 abstract={In this article, two major domestic B2C websites-JD.com and Tmall are used as research objects to compare and analyze the impact of different evaluation systems on sales. The conclusion shows that for different evaluation systems, reviews have different effects on sales. Under the classification evaluation system, the total number of reviews and positive comments have a significant positive impact on sales; the negative evaluation has a significant negative impact on sales. Under the evaluation system without classification, the sales are significantly positively affected by the total number of comments and positive comments. Impact, but the negative evaluation does not affect sales. Price is an important factor affecting sales. The total number of reviews and prices of the two websites have a significant mutual impact on sales. This article analyzes the impact of different online evaluation systems on sales. With a view to providing a reference for enterprises to improve their online evaluation system.},
}

@article{SongPeng,
  title={The Impact of Heterogeneous Online Comment Features on Product Sales},
  author={Song Peng and Guo Qinqin},
  year={2019},
 abstract={Facing the new social media context with user-generated content as the core, the impact of online reviews on consumer purchasing decisions is becoming increasingly prominent. Taking heterogeneous products as research objects, using the Scrapy crawler framework and Redis database to collect Amazon China product reviews as research samples, to explore how the attribute characteristics of online reviews affect the sales of search-type products and experience-type products, and to further develop the network word-of-mouth theory It has important research value. Studies have shown that the impact of comment extremeness and comment length is more pronounced on the sales of search-type products; the impact of the number of reviews is more pronounced on experiential products. The research results help consumers effectively identify useful review features and help e-commerce companies to build efficient online review systems.},
}

