@article{WangGongPeng,
  title={Stochastic gradient descent algorithm based on convolutional neural network},
  author={Wang Gongpeng and Duan Meng and Niu Changyong},
  journal={Computer Engineering and Design},
  volume={39},
  number={2},
  pages={441-445},
  year={2018},
 keywords={Convolutional neural network; Stochastic gradient descent algorithm; Adaptive learning rate update algorithm; LeakyRelu activation function; Fast convergence},
 abstract={In order to solve the impact of improper setting of the learning rate of the stochastic gradient descent algorithm (SGD) on the convolutional neural network (CNN) on the SGD algorithm, an update algorithm of adaptive learning rate of SGD is proposed. As the iteration progresses, the algorithm makes the learning rate Shows periodic changes. Aiming at the defect that the Relu activation function in CNN discards neurons with a negative threshold in CNN, a CNN with LeakyRelu as the activation function is designed. Experiments have verified the effectiveness of using this activation function.The experimental results show that SGD using the above-mentioned learning rate update algorithm can make the network quickly converge and improve the learning accuracy rate; the LeakyRelu activation function and the SGD using the above-mentioned learning rate update algorithm are compared. Combined to further improve the accuracy of CNN learning.},
}

